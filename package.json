{
  "name": "weatherflow",
  "version": "0.1.0",
  "description": "An app that fetches and stores weather forecasts for the user.",
  "main": "app.js",
  "scripts": {
    "test": "npm run clean && jest",
    "start": "nodemon app.js",
    "clean": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "build": "parcel build ./src/index.html --out-dir public --public-url / --no-minify"
  },
  "jest": {
    "roots": [
      "tests"
    ]
  },
  "engines": {
    "node": "9.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braden337/weatherflow.git"
  },
  "bugs": {
    "url": "https://github.com/braden337/weatherflow/issues"
  },
  "homepage": "https://github.com/braden337/weatherflow#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "knex": "^0.14.4",
    "mysql": "^2.15.0",
    "node-fetch": "^2.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.0.0",
    "jest": "^22.4.2",
    "moment": "^2.21.0",
    "node-mocks-http": "^1.6.7",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "parcel-bundler": "^1.6.2",
    "vue": "^2.5.13"
  }
}
