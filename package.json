    "postinstall": "knex migrate:latest"
  },
  "jest": {
    "roots": [
      "tests"
    ]
  },
  "engines": {
    "node": "9.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braden337/weatherflow.git"
  },
  "bugs": {
    "url": "https://github.com/braden337/weatherflow/issues"
  },
  "homepage": "https://github.com/braden337/weatherflow#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "knex": "^0.14.4",
    "mysql": "^2.15.0",
    "node-fetch": "^2.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "jest": "^22.4.2",
    "node-mocks-http": "^1.6.7",
    "nodemon": "^1.17.1"
  }
